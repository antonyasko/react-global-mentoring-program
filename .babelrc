{
  "presets": ["@babel/preset-env", "@babel/preset-react", "@babel/preset-typescript"],
  "plugins": [
    [
      "babel-plugin-tsconfig-paths",
      {
        "relative": true,
        "extensions": [
          ".js",
          ".ts",
          ".tsx"
        ],
        "rootDir": ".",
        "tsconfig": "tsconfig.json"
      }
    ],
    "@babel/plugin-proposal-class-properties", 
    "@babel/plugin-syntax-dynamic-import"
  ]
}
